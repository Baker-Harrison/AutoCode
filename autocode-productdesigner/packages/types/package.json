{
  "name": "@autocode/types",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./ipc": {
      "types": "./dist/ipc.d.ts",
      "import": "./dist/ipc.js",
      "require": "./dist/ipc.js",
      "default": "./dist/ipc.js"
    },
    "./settings": {
      "types": "./dist/settings.d.ts",
      "import": "./dist/settings.js",
      "require": "./dist/settings.js",
      "default": "./dist/settings.js"
    },
    "./git": {
      "types": "./dist/git.d.ts",
      "import": "./dist/git.js",
      "require": "./dist/git.js",
      "default": "./dist/git.js"
    },
    "./shared": {
      "types": "./dist/shared.d.ts",
      "import": "./dist/shared.js",
      "require": "./dist/shared.js",
      "default": "./dist/shared.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
